        /** Background Shorthand: 1. Color 2. Image 3. Repeat 4. Attachment 5. Position **/
        #loading {
          background: url('http://static.coronite.net/img/icons/ring.gif') no-repeat fixed center;
          width:100%;
          height:100%;
          margin:0 auto;
          text-align:center;
          position: fixed;
          transition: all 1s linear;
          z-index:1000;
        }
        .visuallyhidden {
          opacity: 0;
        }
        .visuallyshown {
          opacity: 1;
        }
        .hidden {
          display: none;
        }
        .shown {
          display: block;
        }

        .animated {
          -webkit-animation-duration: 1s;
          animation-duration: 1s;
          -webkit-animation-fill-mode: both;
          animation-fill-mode: both;
        }

        @keyframes fadeInLeft {
          from {
            opacity: 0;
            transform: translate3d(-100%, 0, 0);
          }

          to {
            opacity: 1;
            transform: none;
          }
        }

        .fadeInLeft {
          animation-name: fadeInLeft;
        }

        @keyframes fadeInRight {
          from {
            opacity: 0;
            transform: translate3d(100%, 0, 0);
          }

          to {
            opacity: 1;
            transform: none;
          }
        }

        .fadeInRight {
          animation-name: fadeInRight;
        }

        @keyframes fadeInUp {
          from {
            opacity: 0;
            transform: translate3d(0, 100%, 0);
          }

          to {
            opacity: 1;
            transform: none;
          }
        }

        .fadeInUp {
          animation-name: fadeInUp;
        }

